{
    "name": "Movie Randomizer",
    "description": "Don't know what to watch? I will choose for you!",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage"],
    "action": {
      "default_popup": "main.html",
      "default_icon": {
        "16": "/imgs/icon_16.png",
        "32": "/imgs/icon_32.png",
        "48": "/imgs/icon_48.png",
        "128": "/imgs/icon_128.png"
      }
    },
    "icons": {
        "16": "/imgs/icon_16.png",
        "32": "/imgs/icon_32.png",
        "48": "/imgs/icon_48.png",
        "128": "/imgs/icon_128.png"
    }
}